<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BuilderMenuController">
   <center>
      <BorderPane prefHeight="600.0" prefWidth="800.0">
         <center>
            <AnchorPane maxHeight="300.0" maxWidth="400.0" minHeight="300.0" minWidth="400.0" prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: c0c0c0;" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
               <children>
                  <Button fx:id="btnNewMap" layoutX="128.0" layoutY="8.0" mnemonicParsing="false" onAction="#clickNewMap" prefHeight="50.0" prefWidth="150.0" style="-fx-font-size: 20;" text="New Map" />
                  <Button fx:id="btnLoad" layoutX="128.0" layoutY="64.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" style="-fx-font-size: 20;" text="Load" />
                  <Button fx:id="btnBack" layoutX="128.0" layoutY="120.0" mnemonicParsing="false" onAction="#clickBack" prefHeight="50.0" prefWidth="150.0" style="-fx-font-size: 20;" text="Back" />
               </children>
            </AnchorPane>
         </center>
         <top>
            <Label style="-fx-font-size: 100;" text="Builder Menu" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets top="50.0" />
               </BorderPane.margin>
            </Label>
         </top>
      </BorderPane>
   </center>
</BorderPane>
