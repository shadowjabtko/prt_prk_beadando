<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GameController">
   <center>
      <AnchorPane fx:id="anchorPane" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: c0c0c0;" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </BorderPane.margin>
      </AnchorPane>
   </center>
   <left>
      <BorderPane prefHeight="600.0" prefWidth="154.0" BorderPane.alignment="CENTER">
         <bottom>
            <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onClickBack" prefHeight="55.0" prefWidth="135.0" text="Main Menu" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="20.0" />
               </BorderPane.margin></Button>
         </bottom>
         <center>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Label layoutX="14.0" layoutY="31.0" text="Red points:" />
                  <Text fx:id="redPoints" layoutX="86.0" layoutY="44.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                  <Label layoutX="14.0" layoutY="56.0" text="Greenpoints:" />
                  <Text fx:id="greenPoints" layoutX="86.0" layoutY="69.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                  <Text fx:id="txtInfo" layoutX="14.0" layoutY="108.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 15;" text="Text" />
               </children>
            </AnchorPane>
         </center>
      </BorderPane>
   </left>
</BorderPane>
