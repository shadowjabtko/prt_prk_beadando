<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenuController">
   <center>
      <BorderPane prefHeight="600.0" prefWidth="800.0">
         <center>
            <AnchorPane maxHeight="300.0" maxWidth="400.0" minHeight="300.0" minWidth="400.0" prefHeight="200.0" prefWidth="400.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
               <children>
                  <Button fx:id="btnGame" layoutX="128.0" layoutY="8.0" mnemonicParsing="false" onAction="#clickGame" prefHeight="50.0" prefWidth="150.0" style="-fx-font-size: 20;" text="Game" />
                  <Button fx:id="btnBuilder" layoutX="128.0" layoutY="64.0" mnemonicParsing="false" onAction="#clickBuilder" prefHeight="50.0" prefWidth="150.0" style="-fx-font-size: 20;" text="Builder" />
               </children>
            </AnchorPane>
         </center>
         <top>
            <Label style="-fx-font-size: 100;" text="Main Menu" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets top="50.0" />
               </BorderPane.margin>
            </Label>
         </top>
      </BorderPane>
   </center>
</BorderPane>
